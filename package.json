{
  "name": "remembot",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "check-types": "turbo run check-types",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "postinstall": "find apps -name '.env.example' -exec sh -c 'dir=$(dirname \"{}\"); [ ! -f \"$dir/.env\" ] && cp \"{}\" \"$dir/.env\"' \\;",
    "print-changed-packages": "bun run scripts/print_changed_packages.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.3",
    "prettier": "^3.5.3",
    "turbo": "^2.5.2",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.11",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
